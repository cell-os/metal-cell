{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "skynetservices/skydns:latest", 
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 53,
          "hostPort": 53,
          "protocol": "udp"
        }
      ]
    },
    "volumes" : []
  },
  "env": {
    "ETCD_MACHINES": "http://scvm1231.dev.ut1.omniture.com:4001,http://scvm1232.dev.ut1.omniture.com:4001,http://scvm1235.dev.ut1.omniture.com:4001", 
    "SKYDNS_NAMESERVERS": "10.27.56.31:53,10.27.56.32:53", 
    "SKYDNS_ADDR": "0.0.0.0:53"
  },
  "id": "/skydns",
  "instances": "18",
  "cpus": "1",
  "constraints": [
    ["hostname", "UNIQUE"]
  ],
  "mem": "128",
  "uris": []
}
 
